
<%= form_for(@project, html: { class: "form-horizontal", role: "form" }) do |f| %>
  <% if @project.errors.any? %>
    <div class="alert alert-danger alert-dismissable" role="alert">
      <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
      <h4><%= pluralize(@project.errors.count, "error") %> prohibited this project from being saved:</h4>
      <ul>
      <% @project.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  
  <div class="form-group">
            <%= f.label :name, class: "col-sm-2 control-label" %>
        <div class="col-sm-10">
        <%= f.text_field :name, class: "form-control" %>
      </div>
        </div>
  <div class="form-group">
            <%= f.label :number, class: "col-sm-2 control-label" %>
        <div class="col-sm-10">
        <%= f.text_field :number, class: "form-control" %>
      </div>
        </div>
  <div class="form-group">
            <%= f.label :tech_radar_num, class: "col-sm-2 control-label" %>
        <div class="col-sm-10">
        <%= f.text_field :tech_radar_num, class: "form-control" %>
      </div>
        </div>
  <div class="form-group">
            <%= f.label :active, class: "col-sm-2 control-label" %>
        <div class="col-sm-10">
        <%= f.text_field :active, class: "form-control" %>
      </div>
        </div>
  <div class="form-group">
            <%= f.label :priority, class: "col-sm-2 control-label" %>
        <div class="col-sm-10">
        <%= f.text_field :priority, class: "form-control" %>
      </div>
        </div>
  <div class="form-group">
            <%= f.label :ae_lead, class: "col-sm-2 control-label" %>
        <div class="col-sm-10">
        <%= f.text_field :ae_lead, class: "form-control" %>
      </div>
        </div>
  <div class="form-group">
            <%= f.label :bd_lead_si_customer, class: "col-sm-2 control-label" %>
        <div class="col-sm-10">
        <%= f.text_field :bd_lead_si_customer, class: "form-control" %>
      </div>
        </div>
  <div class="form-group">
            <%= f.label :ae_objective, class: "col-sm-2 control-label" %>
        <div class="col-sm-10">
        <%= f.text_area :ae_objective, class: "form-control" %>
      </div>
        </div>
  <div class="form-group">
            <%= f.label :sort_order, class: "col-sm-2 control-label" %>
        <div class="col-sm-10">
        <%= f.number_field :sort_order, class: "form-control" %>
      </div>
        </div>
  <div class="form-group">
            <%= f.label :active_status, class: "col-sm-2 control-label" %>
        <div class="col-sm-10">
        <%= f.number_field :active_status, class: "form-control" %>
      </div>
        </div>

  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <%= f.submit class: "btn btn-primary" %>
    </div>
  </div>
<% end %>
